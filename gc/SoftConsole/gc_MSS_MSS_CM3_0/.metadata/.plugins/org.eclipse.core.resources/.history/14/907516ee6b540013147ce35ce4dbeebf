#include "xbee_interface.h"
#include "xbee.h"
#include "stdio.h"

uint8_t message[] = {0x48, 0x45, 0x4C, 0x4C, 0x4F};

int main() {
	int err;
	struct xbee_packet * myPacket;
	err = xbee_interface_init();
	if (err != 0) {
		printf("Unable to successfully initialize xbee interface. Error: %d", err);
		while (1) ;
	}

	while (1) {
		myPacket = xbee_interface_create_packet();
		if (!myPacket) {
			printf("Unable to create a new packet");
			while (1) ;
		}
	}



}
